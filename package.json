{
  "name": "sedai",
  "version": "0.1.3",
  "description": "Spec-Exact Development by AI - A development methodology where AI develops exactly as the spec defines",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sedai": "dist/cli.js",
    "spec": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:prod": "bash tests/prod/test.sh",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "development",
    "spec",
    "specification",
    "sed",
    "ai-assisted",
    "development-methodology",
    "cli"
  ],
  "author": "송재호 <thruthesky@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "prompts": "^2.4.2",
    "yaml": "^2.5.0"
  }
}
